{
  "name": "todo-manager",
  "version": "1.0.0",
  "description": "Manager the daily tasks with react, redux, typescript",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "start-server": "npx ts-node server/index.ts",
    "build-server": "cd server && tsc index.ts",
    "pm2": "NODE_ENV=production pm2 start --name 'BackendAPI' server/index.js --watch -i max",
    "start-static-storybook": "npx http-server ./storybook-static",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest --testPathPattern=cypress/jest",
    "test:unit": "jest --watchAll --testPathPattern=cypress/unit",
    "test:integration": "jest --watchAll --testPathPattern=cypress/integration",
    "test:helpers": "jest --watchAll --findRelatedTests cypress/helpers/helpers.test.js",
    "test:e2e": "cypress open",
    "test:coverage": "jest --watchAll=false --testPathPattern=cypress/ --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile"
  },
  "keywords": [
    "daily tasks manager"
  ],
  "author": "Mahyar Hossein Eki",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.4",
    "start-server-and-test": "^1.12.6",
    "styled-components": "^5.3.0",
    "ts-node": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@cypress/code-coverage": "^3.9.8",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/components": "^6.3.0",
    "@storybook/core-events": "^6.3.0",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "6.3.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.17",
    "@types/styled-components": "^5.1.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "browserslist": "^4.16.6",
    "css-what": "^5.0.1",
    "cypress": "^7.7.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob-parent": "^6.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "4",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "trim": "^1.0.1",
    "typescript": "^4.3.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "ws": "^7.5.2",
    "yarn-audit-fix": "^6.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "**/trim": "^1.0.1",
    "**/css-what": "^5.0.1",
    "**/glob-parent": "^6.0.0",
    "**/browserslist": "^4.16.6",
    "**/ws": "^7.5.2",
    "**/@storybook/theming": "6.3.4",
    "**/@storybook/core-events": "^6.3.0",
    "**/@storybook/components": "^6.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
